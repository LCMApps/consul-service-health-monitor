{
  "name": "consul-service-health-monitor",
  "version": "2.2.0",
  "description": "Service to monitor health status of services in consul.",
  "main": "index.js",
  "engines": {
    "node": ">=8.0"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./",
    "tests": "yarn run unit-tests && yarn run func-tests",
    "test:coverage": "NODE_ENV=test nyc --reporter=text --reporter=text-summary mocha --recursive --config tests/.mocharc.json ./tests",
    "unit-tests": "NODE_ENV=test mocha --config tests/.mocharc.json './tests/Unit/**/*.js'",
    "func-tests": "NODE_ENV=test mocha --config tests/.mocharc.json './tests/Functional/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LCMApps/consul-service-health-monitor.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "chai": "4.5.0",
    "chai-as-promised": "8.0.1",
    "consul": "2.0.1",
    "data-driven": "^1.3.0",
    "deep-freeze": "0.0.1",
    "eslint": "^4.4.1",
    "get-port": "5.1.1",
    "mocha": "11.7.1",
    "nock": "13.2.9",
    "nyc": "17.1.0",
    "proxyquire": "2.1.3",
    "sinon": "13.0.2"
  },
  "dependencies": {
    "lodash": "4.17.21"
  }
}
